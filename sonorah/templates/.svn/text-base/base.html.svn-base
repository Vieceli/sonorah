<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">

<head>
    <link rel="shortcut icon" href="/media/images/favicon.ico" >

    <title>{% block titulo %}{% endblock titulo %}Cupon Clipper</title>
    <meta content="text/html; charset=utf-8" http-equiv="Content-Type"/>
    <meta content="{{ meta_keywords }}" name="Keywords"/>
    <meta content="{{ meta_description }}" name="Description"/>
    <meta content="pt-br" http-equiv="Content-Language"/>
    <meta name="verify-v1" content="l4Z4jgwG5X58psxFkH3eILv9ua3nJKYrq5zGFZC0xj0=" />
    {% block metadata %}
    {% endblock metadata %}

    <link type="text/css" rel="stylesheet" href="/media/css/estilo.css" />

    {% block extralinks %}
    {% endblock extralinks %}
    <style type="text/css">
	    {% block extracss %}
	    {% endblock extracss %}
    </style>
    <script type="text/javascript" src="/media/js/jquery-1.3.2.min.js" charset="utf-8"></script>

    {% block extrascripts %}
    {% endblock extrascripts %}
    
 
    <script language="javascript">
	  $(document).ready(function()
	  {
	    $("#user-msg-close").click(function () {
	      $("#user-msg-div").hide();
	      return false;
	    });
	
	    $("#cidade-slider-link").click(function () {
	      $(".cidades-slider").slideToggle("slow");
	      return false;
	    });
	
	    
	  });

    {% block extrajs %}
    {% endblock extrajs %}

    </script>


</head>

<body>
	<div id="conteiner" class="conteiner" >
		{% block cabecalho %}
			{% include "cabecalho.html" %}
		{% endblock cabecalho %}
	
		
		{% block conteudo %}
		{% endblock %}
		
		 
		 {% block rodape %}
		 	{% include "rodape.html" %}
		 {% endblock rodape %}
		       
	</div>
	


</body>
</html>

