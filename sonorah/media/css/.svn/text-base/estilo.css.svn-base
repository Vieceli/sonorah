body{
	margin:0;
	color:#000;
	font:12px Arial, Helvetica, sans-serif;
	background:#4d4d4d;
	min-width:1000px;
}
img{border-style:none;}
a{
	text-decoration:none;
	color:#0881d5;
}
a:hover{text-decoration:underline;}

#conteiner{
	width: 50%;
	left: 25%;
	float: left;
	position: relative
	}
#conteudo{
	width:100%;
	overflow:hidden;
	position:relative;
	float:left;
	background-color: orange;
	/* padding:0 0 49px; */
}
#principal{
	width:100%;
	overflow:hidden;
	padding:0 0 17px;
	background:#d9ebf0 url(../images/bg-main.jpg) no-repeat 50% 153px;
	display:table;
}
#cabecalho{
	width:100%;
	background-color: silver;
	display: inline;
	float: left;
}
#logo{
	width: 20%;
	background-color: teal;
	position: relative;
	float: left;
	height: 50px;
	}
#cidades{
	width:100%;
	background-color: aqua;
	
	}
#menu{
	height: 50px;
	float: right;
	width: 80%;
	background-color: threedlightshadow;
	}

