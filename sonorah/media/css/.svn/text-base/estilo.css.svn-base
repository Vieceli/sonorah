.cidade-slider li {
  display:block;
  float:left;
  padding:10px 0 10px 30px;
  width:129px;
  color: #555;
  font-size:1.2em;

  list-style-type: none;

  white-space:nowrap;

  list-style-image:none;
  list-style-position:outside;
}

.cidade-slider  a {
  display:block;
  float:left;
  padding:10px 0 10px 30px;
  width:129px;

  border-style:none dashed dashed none;
  font-size:1.2em;
	
  border-color: #999;
  color: #fff;
	
  list-style-type: none;
	
   white-space:nowrap;
	
   list-style-image:none;
   list-style-position:outside;
}

#empresa{
	width: 100%;
	text-indent: 5px;
	background-color: yellow;
	float: left;
	position: relative;
	height: 200px;
	}
#video_empresa{
	width: 50%;
	text-indent: 5px;
	background-color: black;
	float: right;
	position: relative;
	height: 100%;
	}
#agende_visita{
	width: 100%;
	text-indent: 5px;
	
	float: right;
	position: relative;
	height: 80px;
	
}
#edicao_atual{
	float: left;
	position: relative;
	width: 100%;
	
	}
#revista_edicao{
	display: table;
	border-style:solid;
	
	}
#rodape{
	background-color: lime;
	position: relative;
	clear: both;
	width: 100%;
	}